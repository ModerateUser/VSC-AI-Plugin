{
  "name": "vscode-agentic-workflow",
  "displayName": "Agentic Workflow Plugin",
  "description": "Visual node-based workflow engine for VS Code with AI model management, automation, and vector database support",
  "version": "0.2.0",
  "publisher": "ModerateUser",
  "author": {
    "name": "ModerateUser",
    "url": "https://github.com/ModerateUser"
  },
  "license": "MIT",
  "icon": "resources/workflow-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ModerateUser/VSC-AI-Plugin.git"
  },
  "bugs": {
    "url": "https://github.com/ModerateUser/VSC-AI-Plugin/issues"
  },
  "homepage": "https://github.com/ModerateUser/VSC-AI-Plugin#readme",
  "keywords": [
    "workflow",
    "automation",
    "ai",
    "machine-learning",
    "visual-programming",
    "node-based",
    "huggingface",
    "vector-database",
    "github-actions",
    "agentic"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:agenticWorkflow.openEditor",
    "onCommand:agenticWorkflow.executeWorkflow",
    "onCommand:agenticWorkflow.manageModels"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agenticWorkflow.openEditor",
        "title": "Open Workflow Editor",
        "category": "Agentic Workflow"
      },
      {
        "command": "agenticWorkflow.executeWorkflow",
        "title": "Execute Workflow",
        "category": "Agentic Workflow"
      },
      {
        "command": "agenticWorkflow.manageModels",
        "title": "Manage AI Models",
        "category": "Agentic Workflow"
      },
      {
        "command": "agenticWorkflow.createWorkflow",
        "title": "Create New Workflow",
        "category": "Agentic Workflow"
      },
      {
        "command": "agenticWorkflow.importWorkflow",
        "title": "Import Workflow",
        "category": "Agentic Workflow"
      },
      {
        "command": "agenticWorkflow.exportWorkflow",
        "title": "Export Workflow",
        "category": "Agentic Workflow"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agenticWorkflow",
          "title": "Agentic Workflow",
          "icon": "resources/workflow-icon.svg"
        }
      ]
    },
    "views": {
      "agenticWorkflow": [
        {
          "id": "workflowExplorer",
          "name": "Workflows"
        },
        {
          "id": "modelManager",
          "name": "AI Models"
        },
        {
          "id": "executionHistory",
          "name": "Execution History"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "agenticWorkflow.editor",
        "displayName": "Workflow Editor",
        "selector": [
          {
            "filenamePattern": "*.workflow.json"
          },
          {
            "filenamePattern": "*.workflow.yaml"
          },
          {
            "filenamePattern": "*.workflow.yml"
          }
        ],
        "priority": "default"
      }
    ],
    "configuration": {
      "title": "Agentic Workflow",
      "properties": {
        "agenticWorkflow.huggingFaceToken": {
          "type": "string",
          "default": "",
          "description": "Hugging Face API token for model downloads"
        },
        "agenticWorkflow.githubToken": {
          "type": "string",
          "default": "",
          "description": "GitHub token for Actions integration"
        },
        "agenticWorkflow.modelCachePath": {
          "type": "string",
          "default": "",
          "description": "Custom path for model cache (leave empty for default)"
        },
        "agenticWorkflow.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Path to Python executable for embedding generation"
        },
        "agenticWorkflow.maxConcurrentNodes": {
          "type": "number",
          "default": 5,
          "description": "Maximum number of nodes to execute concurrently"
        },
        "agenticWorkflow.enableVectorDB": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic vector database generation"
        },
        "agenticWorkflow.enableGPU": {
          "type": "boolean",
          "default": false,
          "description": "Enable GPU acceleration for AI models (requires CUDA/ROCm/Metal)"
        },
        "agenticWorkflow.gpuDevice": {
          "type": "number",
          "default": 0,
          "description": "GPU device ID to use (0 = first GPU, -1 = CPU only)"
        },
        "agenticWorkflow.gpuMemoryLimit": {
          "type": "number",
          "default": 0,
          "description": "GPU memory limit in GB (0 = no limit)"
        },
        "agenticWorkflow.gpuBatchSize": {
          "type": "number",
          "default": 32,
          "description": "Batch size for GPU inference (higher = faster but more memory)"
        },
        "agenticWorkflow.enableMixedPrecision": {
          "type": "boolean",
          "default": true,
          "description": "Use FP16 mixed precision for faster inference (requires GPU)"
        },
        "agenticWorkflow.gpuMemoryFraction": {
          "type": "number",
          "default": 0.9,
          "minimum": 0.1,
          "maximum": 1.0,
          "description": "Fraction of GPU memory to use (0.1-1.0)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.9",
    "@types/uuid": "^9.0.7",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@huggingface/inference": "^2.6.4",
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.5",
    "faiss-node": "^0.5.1",
    "immer": "^10.0.3",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.7.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  }
}
